{
    "name": "foxsocial/package-bundler-plugin",
    "type": "composer-plugin",
    "version": "1.0.4",
    "description": "Bundle multiple packages into root package",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2.0",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "files": ["helpers.php"],
        "psr-4": {
            "FoxSocial\\PackageBundlerPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FoxSocial\\PackageBundlerPlugin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "class": "FoxSocial\\PackageBundlerPlugin\\Bundler"
    }
}
